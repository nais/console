"""
Metadata describing the response data.
"""
type Meta {
    num_results: Int!
}

type User {
    id: UUID!
    email: String
    name: String!
    teams: [Team!]!
    roles: [Role!]!
    created_at: Time
}

type Team {
    id: UUID!
    slug: String!
    name: String!
    purpose: String
    roles: [Role!]!
    users: [User!]!
    metadata: [TeamMetadata!]!
}

type Role {
    id: UUID!
}

type TeamMetadata {
    id: UUID!
}
